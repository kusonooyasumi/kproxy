{
	"name": "kproxy",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"main": "electron/main.js",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"electron:dev": "concurrently -k \"cross-env NODE_ENV=development vite dev\" \"wait-on http://localhost:5173 && electron .\"",
		"electron:build": "vite build",
		"electron:preview": "vite build && electron .",
		"electron:package": "electron-builder build --config electron-builder.config.js"
	},
	"devDependencies": {
		"@codemirror/lang-javascript": "^6.2.3",
		"@codemirror/theme-one-dark": "^6.1.2",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/highlight.js": "^10.1.0",
		"@types/marked": "^6.0.0",
		"concurrently": "^9.1.2",
		"cross-env": "^7.0.3",
		"electron": "^35.0.0",
		"electron-builder": "^25.1.8",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.0.0",
		"wait-on": "^8.0.2"
	},
	"dependencies": {
		"@rollup/rollup-win32-x64-msvc": "^4.34.9",
		"@tailwindcss/oxide-win32-x64-msvc": "^4.0.11",
		"deep-chat": "^2.1.1",
		"electron-store": "^8.1.0",
		"highlight.js": "^11.11.1",
		"http-proxy": "^1.18.1",
		"lightningcss-win32-x64-msvc": "^1.29.2",
		"marked": "^15.0.7",
		"node-forge": "^1.3.1",
		"svelte-codemirror-editor": "^1.4.1",
		"svelte-splitpanes": "^8.0.9"
	}
}
