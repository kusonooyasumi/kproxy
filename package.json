	"main": ".vite/main/main.js",
	"type": "module",
	"scripts": {
		"start": "tsx node_modules/@electron-forge/cli/src/electron-forge-start",
		"package": "tsx node_modules/@electron-forge/cli/src/electron-forge-package",
